{
    "emmet.excludeLanguages": [],
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "markdown": "html",
        "javascript": "javascriptreact"
    },
    "files.exclude": {
        "**/*.js.map": {
            "when": "$(basename)"
        },
    },
    "html.autoClosingTags": true,
    "javascript.autoClosingTags": true,
    "javascript.suggest.autoImports": true,
    "search.exclude": {
        "**/coverage": true,
        "**/node_modules": true
    },
    "cSpell.words": [
        "Dsonar",
        "Qube",
        "Sonarqube",
        "avif",
        "dein",
        "lcov",
        "sonarcloud",
        "sonarsource",
        "testid"
    ],
}